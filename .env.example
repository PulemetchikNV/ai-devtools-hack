
# === MCP + Postgres (docker-compose.mcp.yml) ===
PORT=3000
DATABASE_URL=postgresql://postgres:postgres@db:5432/mcp_gitlab?schema=public
ENCRYPTION_KEY=change-me-hex-64
API_KEY=change-me-api-key

# === Агент (docker-compose.yml) ===
LLM_MODEL=gpt-4.1
LLM_API_BASE=https://api.openai.com/v1
LLM_API_KEY=your-llm-key
AGENT_NAME=Main Agent
AGENT_DESCRIPTION=AI Assistant Agent
AGENT_VERSION=v1.0.0
AGENT_SYSTEM_PROMPT=Ты полезный AI-ассистент. Используй доступные инструменты для решения задач пользователя.
MCP_URL=http://mcp-gitlab-server:3000/mcp
PHOENIX_ENDPOINT=
ENABLE_PHOENIX=false
LOG_LEVEL=INFO
AGENT_PORT=10000

# === Telegram-бот (docker-compose.yml) ===
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
ADMIN_CHAT_ID=1234567890
A2A_AGENT_URL=http://base-agent:10000

# === Публикация образов в Artifact Registry (publish-*.sh) ===
REGISTRY=mcp-gitlab-server.cr.cloud.ru
IMAGE_NAME=mcp-gitlab-server
TAG=latest
CLOUDRU_AR_USERNAME=