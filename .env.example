# ===========================================
# Docker Compose Environment Variables
# ===========================================
# Copy this file to .env and fill in the values

# --- Telegram Bot ---
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
ADMIN_CHAT_ID=

# --- LLM Configuration ---
LLM_MODEL=hosted_vllm/Qwen/Qwen3-Coder-480B-A35B-Instruct
LLM_API_BASE=https://foundation-models.api.cloud.ru/v1
LLM_API_KEY=your-llm-api-key

# --- Agent Configuration ---
AGENT_NAME=Main Agent
AGENT_DESCRIPTION=AI Assistant Agent
AGENT_VERSION=v1.0.0
AGENT_SYSTEM_PROMPT=Ты полезный AI-ассистент. Используй доступные инструменты для решения задач пользователя.

# --- Optional: Agent Port (default: 10000) ---
AGENT_PORT=10000

# --- Optional: MCP Servers (comma-separated URLs) ---
MCP_URL=

# --- Optional: Phoenix Telemetry ---
PHOENIX_ENDPOINT=
ENABLE_PHOENIX=false

# --- Optional: Logging ---
LOG_LEVEL=INFO
